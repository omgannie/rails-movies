<h1>New user</h1>

<%= form_for @user do |f| %>
<!-- add errors partial -->
  <p>
    <%= f.label :email %>
    <%= f.email_field :email %>
  </p>
  <p>
    <%= f.label :username %>
    <%= f.text_field :username %>
  </p>
  <p>
    <%= f.label :password %>
    <%= f.password_field(:password) %>
  </p>
  <p>
    <%= f.label :key %>
    <%= f.password_field(:key) %>
  </p>
  <p>
    <%= f.submit %>
  </p>
 <% end %>
